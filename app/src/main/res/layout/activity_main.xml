<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="260dp"
        android:layout_height="160dp"
        android:layout_marginStart="8dp"
        android:contentDescription="@string/logo_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/r4" />

    <ImageView
        android:id="@+id/pushup"
        android:layout_width="250dp"
        android:layout_height="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/pushup_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo"
        app:srcCompat="@drawable/pushup" />

    <ImageView
        android:id="@+id/legs"
        android:layout_width="250dp"
        android:layout_height="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/legs_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pushup"
        app:srcCompat="@drawable/leg" />

    <ImageView
        android:id="@+id/situp"
        android:layout_width="250dp"
        android:layout_height="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/situp_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/legs"
        app:srcCompat="@drawable/situp" />

    <ImageView
        android:id="@+id/apple"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="270dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/fruit_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/apple" />

    <ImageView
        android:id="@+id/date"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="270dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/dried_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/apple"
        app:srcCompat="@drawable/date" />

    <ImageView
        android:id="@+id/nut"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="270dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/nuts_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/date"
        app:srcCompat="@drawable/nut" />

    <ImageView
        android:id="@+id/seed"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="270dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/seed_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/nut"
        app:srcCompat="@drawable/seed" />

    <ImageView
        android:id="@+id/berry"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="270dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/berry_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/seed"
        app:srcCompat="@drawable/berry" />

    <ImageView
        android:id="@+id/misc"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="270dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/misc_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/berry"
        app:srcCompat="@drawable/misc" />

    <ImageView
        android:id="@+id/phi"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/phi_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/situp"
        app:srcCompat="@drawable/fi" />

    <ImageView
        android:id="@+id/clothes"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/clothes_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/situp"
        app:srcCompat="@drawable/cloth" />

    <ImageView
        android:id="@+id/todo"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/todo_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phi"
        app:srcCompat="@drawable/todo" />

    <ImageView
        android:id="@+id/schedule"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/sched_desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/clothes"
        app:srcCompat="@drawable/schedule" />

</androidx.constraintlayout.widget.ConstraintLayout>